<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: yellowgreen" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.baro.MarketingController">
   <children>
      <VBox layoutX="6.0" prefWidth="1100.0" spacing="20.0">
         <children>
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="30.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="title : " />
                  <TextField fx:id="titleInput" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="30.0">
               <children>
                  <Label text="content : " />
                  <TextField fx:id="contentInput" prefWidth="200.0" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#sendFcm" prefHeight="29.0" prefWidth="304.0" text="보내기" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
